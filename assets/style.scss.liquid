        height:100% !important;
      }
    li {
      background-size:cover !important;
      -moz-background-size:cover !important;
      -ms-background-size:cover !important;
      -o-background-size:cover !important;
      -webkit-background-size:cover !important;
      height:100%;
      @include at-query ($medium, $maxMedium){
        height:100% !important;
      }
      @include at-query ($max, $medium) {
      	height:100vh !important;
		
        background-position: center center !important;
      }
      .container{
        position:relative;
        height:100%;
      }

      .slide_overlay{
        background:rgba($colorBlack, 0.3);
        height:100%;
        width:100%;
        position:absolute;
        top:0;
        left:0; 
      }

      .caption{
        left:auto;
        width:400px;
        color:$colorWhite;
        @include vertical-center-content();

        @include at-query($max , $large){
          left:50% !important;
          transform:translate(-50%, -50%) !important;
          -moz-transform:translate(-50%, -50%) !important;
          -webkit-transform:translate(-50%, -50%) !important;
          text-align:center !important;
        }

        @include at-query ($small, $medium) {
          top:68%;
          width:75%;
        }

        @include at-query ($medium, $large) {
          top:55%;
          width:60%;
        }

        @include at-query($max , $small){
          width:75%;
          text-align:center;
          top: 60%;
        }

        h2{
          margin-bottom:16px;
          font-size:36px;
          font-weight:$ubuntu-Normal;
          text-shadow: 2px 2px 2px $colorBlack;

          @include at-query($max , $small){
