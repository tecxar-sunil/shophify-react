  .bx-wrapper{
    .bx-viewport{
      height: 100vh !important;
      @include at-query ($medium, $maxMedium){
        height:100% !important;
      }
    }

    .bx-controls {
      .bx-controls-direction {
        a{
          @include at-query ($max, $medium) {
            width: 25px;
            height: 25px;
            top: 60%;
          }

          @include at-query ($small, $medium) {
            top: 70%;
          }

          @include at-query ($medium, $large) {
            top: 55%;
          }
          
          &.bx-prev {
            left:20px;
            margin-left:0px;
          }
          &.bx-next {
            right: 20px;
            margin-right:0px;
            @include at-query ($max, $medium) {
