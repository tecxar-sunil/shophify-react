          font-weight:$ubuntu-Normal;
          margin-bottom: 33px;

          @include at-query($max , $small){
            font-weight:$ubuntu-Light;
            font-size:16px;
            margin-bottom: 29px;
          }
          @include at-query ($small, $medium) {
            font-size:14px;
            margin-bottom: 12px;
          }
        }

        a{
          background:rgba($colorWhite, 0.15);
          border-color:$colorWhite;
          color:$colorWhite !important;
          text-transform:capitalize;
          font-weight:$ubuntu-Medium;
          font-size:16px;

          span{
            border-color:$colorWhite;
