    //	-webkit-appearance:none;
    background:url(checkbx-ckd.png) no-repeat;
  }
}


/* ============= Radio ============== */

input[type=radio]{
  background:none;
  display: none;

  & + label{
    background:url(radio.png) no-repeat;
    padding-left: 28px;
    line-height: 16px;
    @include transition(all 0.4s ease-in-out);
  }

  &:checked + label{
    border:0;
