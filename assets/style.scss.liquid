          width:100% !important;
          padding:10px;
        }

        @include at-query ($max, $small) {
          height: 258px;
        }

        @include at-query ($small, $medium) {
          height: 306px;
        }

        @include at-query ($medium, $large) {
          width:100% !important;
          height: 397px;
        }

        @include at-query ($large, $maxlarge) {
          height: 445px;
          width: 100% !important;
        }

        img{
          height:100%;
          margin: 0 auto;
          {% if settings.enable_product_image_zoom %}
          cursor: crosshair;
          {% endif %}
        }
      }
    }
    .bx-controls-direction a{
      width: 22px;
      height: 22px;
      &.bx-prev{
        left: 25px;       
      }
      &.bx-next{
        left: auto;
        right: 25px;
         background-position: 0px -22px;
      }
    }
  }

  .product_pager{
    width:170px;
    float:right;

    @include at-query ($max, $small) {
      width: 100%;
      float:none;
      margin-top:20px;
      display:none;
    }

    @include at-query ($small, $medium) {
      width: 13%;
      float: right;
    }

    @include at-query ($medium, $large) {
      width: 13%;
    }

    @include at-query ($large, $maxlarge) {
      width: 104px;
    }

    #bx-pager{
      @include at-query ($max, $small){
        display: none;
      }
    }

    .bx-controls-direction{
      opacity: 0;
      @include transition(0.4s all ease-in-out);

      .bx-prev{
        top: -24px;
        border: none;
        left: 60px;
        @include transform(rotate(90deg));
        @include transition(0.4s all ease-in-out);
      }

      .bx-next{
        right: 60px;
        top: auto;
        border: none;
        bottom: -42px;
        @include transform(rotate(90deg));
        @include transition(0.4s all ease-in-out);
      }
    }

    a{
      display:block;
      height:118px;
      padding:4px;
      border:1px solid $colorGray;
      text-align: center;

      @include at-query ($max, $small) {
        height: 68px;
        margin-bottom:0;
        display:inline-block;
        margin-right: 2px;
        width: 68px;
      }

      @include at-query ($small, $medium) {
        height: 70px;
        margin-bottom: 9px;
        display: inline-block;
        margin-right: 2px;
        width: 70px;
      }

      @include at-query ($medium, $large) {
        height: 91px;
      }

      @include at-query ($large, $maxlarge) {
        height: 103px;
      }

      &:last-child{
        margin:0;
      }

      img{
        height:100%;
        margin: 0 auto;
      }
    }

    &:hover{
      .bx-controls-direction{
        opacity:1;

        .bx-prev{
          top: -4px;
        }
        .bx-next{
          bottom: -22px;
        }
      }
    }
  }
}

