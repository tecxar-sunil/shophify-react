  }
}

.change-view--active {
  cursor: default;
  color: $colorPrimary;
}

/*============================================================================
#Rich Text Editor
==============================================================================*/
.rte {
  margin-bottom: $gutter / 2;

  a {
    text-decoration: underline;
  }

  // Add some top margin to headers from the rich text editor
  h1, h2, h3, h4, h5, h6 {
