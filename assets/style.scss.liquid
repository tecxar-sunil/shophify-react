    margin-bottom: $gutter / 2;
  }
}

.rte--header {
  margin-bottom: 0;
}

/*============================================================================
#Links and Buttons
==============================================================================*/
a,
.text-link {
  color: $colorLink;
  text-decoration: none;
  background: transparent;
}

a:hover,
a:focus {
  color: $colorLinkHover;
}

button {
  overflow: visible;
}

button[disabled],
html input[disabled] {
  cursor: default;
}

.btn,
.rte .btn {
  display: inline-block;
  //padding: 13px 25px;
  padding: 0 25px;
  // font-weight:$ubuntu-Medium;
  border: 1px solid $colorPrimary;
  width: auto;
  margin: 0;
  line-height: 45px;
  height: 45px;
  font-size:16px;
  text-decoration: none;
  text-align: center;
  @include border_radius(4px);
  vertical-align: middle;
  white-space: nowrap;
  cursor: pointer;
  text-transform:uppercase;
  background:none;
  -webkit-appearance: none;
  -moz-appearance: none;
  color: $colorPrimary !important;
  @include transition(all 0.4s ease-in-out);

  @include at-query ($max, $medium) {
    padding: 0 14px;
    font-size: 14px;
  }

  @include at-query ($medium, $large) {
    font-size:14px; 
  }

  span{
    height: 17px;
    width: 17px;
    position: relative;
    top: -1px;
    display: inline-block;
    text-align: center;
    border: 1px solid $colorPrimary;
    color: $colorPrimary;
    border-radius: 25px;
    font-size: 14px;
    margin-left: 13px;
    @include transition(all 0.4s ease-in-out);

    @include at-query ($max, $medium) {
      margin-left: 8px;
    }

    i{
      width: 16px;

      &.fa-angle-down{
        width: 14px;
        height: 16px;
        line-height: 16px;

        @include at-query ($medium, $large) {
          width: 15px;
        }
      }
    }
  }

  &:hover {
    background-color: $colorBtnPrimaryHover;
    color: $colorWhite !important;

    span{
      border-color:$colorWhite;
      color:$colorWhite !important;
    }
  }

  &:active{
    background-color: $colorBtnPrimaryActive;
    color: $colorWhite !important;
  }

  &[disabled],
  &.disabled {
    cursor: default;
    color: $disabledBorder !important;
    background-color: $disabledGrey;
    border-color: $disabledBorder;
    
    &:hover{
      color : $disabledBorder !important;
    }
  }
