/*============================================================================
#Breakpoint and Grid Variables
==============================================================================*/
$viewportIncrement: 1px;

$small: 480px;
$medium: 767px;
$maxMedium: 991px;
$large: 1024px;
$maxlarge:1370px;

$postSmall: $small + $viewportIncrement;
$preMedium: $medium - $viewportIncrement;
$preLarge: $large - $viewportIncrement;
$postmaxlarge: $large + $viewportIncrement;

/*================ The following are dependencies of csswizardry grid ================*/
$breakpoints: (
  'small' '(max-width: #{$small})',
  'medium' '(min-width: #{$postSmall}) and (max-width: #{$medium})',
  'max-medium' '(min-width: #{$medium}) and (max-width: #{$large})',
  'medium-down' '(max-width: #{$medium})',
  'large' '(min-width: #{$large})',
  'max-large' '(min-width: #{$postmaxlarge}) and (max-width: #{$maxlarge})'
);
$breakpoint-has-widths: ('small', 'medium', 'medium-down', 'max-medium' , 'large' , 'max-large');
$breakpoint-has-push:  ('medium', 'medium-down', 'max-medium' , 'max-medium' , 'large' , 'max-large');
$breakpoint-has-pull:  ('medium', 'medium-down', 'max-medium' , 'large' , 'max-large');

