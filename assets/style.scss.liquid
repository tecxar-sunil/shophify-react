  }

  .slider_part{
    border:1px solid $colorGray;
    height:510px;
    width:670px;
    float:left;

    @include at-query ($max, $small) {
      height: 258px;
      width:100%;
      float: none;
    }

    @include at-query ($small, $medium) {
      height: 307px;
      width: 85%;
      float: left;
    }

    @include at-query ($medium, $large) {
      width: 86%;
      height: 400px;
    }

