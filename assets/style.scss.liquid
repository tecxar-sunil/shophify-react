  background: $colorWhite;
  position: fixed;
  top: 0;
  left: 0;
  height: 100%;
  z-index: 99;
  width: 100%;
  display:none;
}


.product_specify .product_detail  input[type=radio]+label{
	padding-left:11px;
}

.spr-badge-caption{
	display:none;
}

.search_section .blog_section{
	background:none;
}

.top_footer{
  position:relative;
  #scrollToTop{
    display:none !important;
    position: fixed;
    bottom: 50px;
    right: 50px;
    border: 1px solid $colorPrimary;
    border-radius: 47px;
    padding: 0px 3px 0px;
    height:20px;
    width:20px; 
    top: auto !important;
    @include at-query($max , $preMedium){
      cursor:pointer;
      background-color:$colorPrimary;
      display: inline-block;
      height: 30px;
      width: 30px;
      color: $colorBody;
      font-size: 30px;
      text-align: center;
      text-decoration: none;
      line-height: 38px;
      position: absolute;
      bottom: 60px;
      right: 0px;
      border-radius: 4px;
      box-shadow: 0px 1px 4px rgb(90, 90, 90);

    }
    span{
      font-size:18px;
      @include at-query($max , $preMedium){
        font-size:28px;
      }
    }
  }
}

.popular_product_section {
	.featured_btn{
    	text-align:center;
    }

}

.wishlist-add-cart{
  @include at-query($max , $small){
    text-align:right;
    .btn{
      height: 32px;
      line-height: 32px;
      font-size: 12px;
    }
  }
}
