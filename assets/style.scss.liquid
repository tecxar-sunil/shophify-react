    display: -webkit-flex;
    display: -ms-flexbox;
    display: flex;
    -webkit-flex-wrap: wrap;
    -ms-flex-flow: row wrap;
    flex-wrap: wrap;

    .grid__item{
      display: -webkit-flex;
      display: -ms-flexbox;
      display: flex;
      -webkit-flex-wrap: wrap;
      -ms-flex-flow: row wrap;
      flex-wrap: wrap;
    }
  }

  .blog_item{
    margin-bottom:40px;

    @include at-query ($max, $medium) {
      padding-bottom: 15px;
      margin-bottom: 8px;
      // border-bottom: 1px solid $colorBorder;
    }

    .blog_img{
      display:block;
      height: 240px;
      overflow: hidden;
      width: 100%;
      position:relative;
      @include transition(all 0.4s ease-in-out);

      @include at-query ($max, $medium) {
        height: 135px;
      }

      &:after{
        z-index: 1;
        content: "";
        position: absolute;
        left: 15px;
        right: 15px;
        top: 15px;
        bottom: 15px;
        @include transition(all 0.4s ease-in-out);

        border-left: 1px solid $colorWhite;
        border-right: 1px solid $colorWhite;
        @include transform(scale(1, 0));
      }

      &:before{
        z-index: 1;
        content: "";
        position: absolute;
        left: 15px;
        right: 15px;
        top: 15px;
        bottom: 15px;
        @include transition(all 0.4s ease-in-out);
        border-top: 1px solid $colorWhite;
        border-bottom: 1px solid $colorWhite;
        @include transform(scale(0, 1));
      }

      &:hover{
        &:after{
          @include transform(scale(1, 1));
        }
        &:before{
          @include transform(scale(1, 1));
        }
      }

      @include at-query ($small, $medium){
        height: 210px;
      }

      @include at-query ($medium, $large){
        height: 170px;
      }

      @include at-query ($large, $maxlarge){
        height: 230px;
      }

      img{
        height:100%;
        width:100%;
        object-fit:cover;
      }
    }

    .blog_desc{
      margin-top: 20px;

      a{
        font-size:18px;
        font-weight:$ubuntu-Medium;
        margin-bottom:10px;
        overflow:hidden;
        display: inline-block;
        @include transition(all 0.3s ease-in-out);

        @include at-query ($medium, $large){
          font-size: 16px;
        }

        @include at-query ($max, $medium) {
          height: auto;
          max-height: auto;
          font-size: 16px;
        }
      }
      p{
        margin-bottom:0;
        max-height:50px;
        overflow:hidden;
        color:darken($colorWhite, 45);

        @include at-query ($medium, $large){
          font-size:14px;
          max-height: 70px;
        }
      }

      h6{
        font-size:13px;
        color:lighten($colorBlack, 20);
        font-weight:$ubuntu-Medium;

        span{
          color:$colorPrimary;
        }
      }
    }
  }
}


