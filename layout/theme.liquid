{% comment %}
  Add the page template as a class for easy page or template specific styling.
{% endcomment %}


<body  data-shop="{{ shop.name }}" id="{{ page_title | handle }}" class="{% if customer %}customer-logged-in {% endif %}template-{{ template | replace: '.', ' ' | truncatewords: 1, '' | handle }}" >

 {% unless template == 'password' %}
  
    {% if settings.newsletter_enable %}
    	<div class="newsletter_popup">

      <div class="newsletter_form">

        <h2>{{ settings.popup_newsletter_title }}</h2>
        <p>{{ settings.popup_descrition }}</p>

        <span class="close_pop"><i class="fa fa-times"></i></span>

        {% if settings.newsletter_form_action != blank %}
          {% assign form_action = settings.newsletter_form_action %}
          <form action="{{ form_action}}" method="post" id="mc-embedded-subscribe-form" name="mc-embedded-subscribe-form" target="_blank" >
            <div class="row">
              <input type="email" placeholder="Enter your E-mail" required>
            </div>
            <div class="submit_row">
              <button type="submit">Subscribe <span><i class="fa fa-angle-right"></i></span></button>
            </div>
          </form>
        {% else %}
          {% form 'customer' %}
            {{ form.errors | default_errors }}
            {% if form.posted_successfully? %}
              <p class="form--success">{{ 'contact.form.post_success' | t }}</p>
            {% else %}
              <input type="hidden" name="contact[tags]" value="newsletter">
              <div class="row">
                <input type="email" class="input-group-field" value="{% if customer %}{{ customer.email }}{% endif %}" placeholder="Enter your E-mail" name="contact[email]" id="mail" required/> 
              </div>
              <div class="submit_row">
                <button type="submit">Subscribe <span><i class="fa fa-angle-right"></i></span></button>
              </div>
            {% endif %}
          {% endform %}
        {% endif %}

      </div>

    </div>
	{% endif %}  
 {% endunless %}
 
 {% if template == '404' or template == 'password' %}
   {{ content_for_layout }}
  {% endif %}

 {% unless template == '404' or template == 'password' %}
 <div id="PageContainer">
    	

   {% include 'header' %}

   {{ content_for_layout }}

   {% include 'footer' %}

</div>
 {% endunless %} 

   {% unless template == '404' or template == 'password'  %}
  <div id="CartDrawer" class="drawer drawer--right">
    <div class="drawer__header">
      <div class="drawer__title h3">{{ 'cart.general.title' | t }}</div>
      <div class="drawer__close js-drawer-close">
        <button type="button" class="icon-fallback-text">
          <span class="icon icon-x" aria-hidden="true"></span>
          <span class="fallback-text">{{ 'cart.general.close_cart' | t | json }}</span>
        </button>
      </div>
    </div>
    <div id="CartContainer"></div>
  </div>
 {% endunless %} 

  {{ 'jquery.history.js' | asset_url | script_tag }}
  {{ 'fastclick.min.js' | asset_url | script_tag }}
  {{ 'chicago.js' | asset_url | script_tag }}
  
  
  {% comment %}
    Template-specific js
  {% endcomment %}
  
  <script>
    {% if newHash %}
      $(function() {
        hulkthemes.updateHash('{{ newHash }}');
      });
    {% endif %}
    {% if resetPassword %}
      $(function() {
        hulkthemes.resetPasswordSuccess();
      });
    {% endif %}
  </script>

  {% comment %}
    Ajaxify your cart with this plugin.
    Documentation:
      - http://shopify.com/plhulkthemesanetx#ajax-cart
  {% endcomment %}

  {% if settings.enable_product_image_zoom %}
    {{ 'jquery.zoom.js' | asset_url | script_tag }}
  {% endif %}
  
  {% if settings.ajax_cart_enable %}
    {{ 'handlebars.min.js' | asset_url | script_tag }}
    
  {% include 'ajax-cart-template' %}
    {{ 'ajax-cart.js' | asset_url | script_tag }}
    <script>
      jQuery(function($) {
        ajaxCart.init({
          formSelector: '#AddToCartForm',
          cartContainer: '#CartContainer',
          addToCartSelector: '#AddToCart',
          cartCountSelector: '#CartCount',
          cartCostSelector: '#CartCost',
          moneyFormat: {{ shop.money_format | json }}
        });
      });

      jQuery('body').on('ajaxCart.afterCartLoad', function(evt, cart) {
        // Bind to 'ajaxCart.afterCartLoad' to run any javascript after the cart has loaded in the DOM
        hulkthemes.RightDrawer.open();
     });
        
       
    </script>
  {% endif %}
  
  

<script>
    
// ========= swatch =============    
jQuery(function() {
  jQuery('.swatch :radio').change(function() {
    var optionIndex = jQuery(this).closest('.swatch').attr('data-option-index');
    var optionValue = jQuery(this).val();
    jQuery(this)
      .closest('form')
      .find('.single-option-selector')
      .eq(optionIndex)
      .val(optionValue)
      .trigger('change');
  });
});
</script>
  


  
 
